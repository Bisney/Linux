alias cls=clear
alias ?=whereis
alias enAES='openssl enc -aes-256-cbc -salt -in'
alias dAES='openssl enc -d -aes-256-cbc -in'
alias Update='sudo apt update && sudo apt upgrade -y && sudo apt autoremove -y'
alias AList='sudo apt list --installed | grep'
alias INET='ifconfig | grep inet'
alias NMAP='nmap -Pn'
alias OSSH='sudo service ssh start'
alias XRDP='sudo service xrdp start'
alias LSA='ls -a'

